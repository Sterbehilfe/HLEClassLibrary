<Project>

  <PropertyGroup>
    <ManagePackageVersionsCentrally>true</ManagePackageVersionsCentrally>
    <CentralPackageTransitivePinningEnabled>true</CentralPackageTransitivePinningEnabled>
  </PropertyGroup>

  <ItemGroup>
    <PackageVersion Include="HLE" Version="$(NugetPackageVersion)"/>
  </ItemGroup>

  <ItemGroup>
    <PackageVersion Include="Microsoft.CodeAnalysis.Common" Version="4.12.0-2.final"/>
    <PackageVersion Include="Microsoft.CodeAnalysis.CSharp" Version="4.12.0-2.final"/>
    <PackageVersion Include="Microsoft.CodeAnalysis.Analyzers" Version="3.11.0"/>
    <PackageVersion Include="Microsoft.Extensions.Http" Version="9.0.0-rc.2.24473.5"/>
    <PackageVersion Include="System.IO.Hashing" Version="9.0.0-rc.2.24473.5"/>
    <PackageVersion Include="System.Text.Json" Version="9.0.0-rc.2.24473.5"/>
  </ItemGroup>

  <!-- Default Analyzers -->
  <ItemGroup>
    <PackageVersion Include="Microsoft.VisualStudio.Threading.Analyzers" Version="17.11.20"/>
    <PackageVersion Include="Roslynator.Analyzers" Version="4.12.7"/>
    <PackageVersion Include="Roslynator.CodeAnalysis.Analyzers" Version="4.12.7"/>
    <PackageVersion Include="Roslynator.CodeFixes" Version="4.12.7"/>
    <PackageVersion Include="Roslynator.Formatting.Analyzers" Version="4.12.7"/>
    <PackageVersion Include="SonarAnalyzer.CSharp" Version="9.32.0.97167"/>
  </ItemGroup>

  <!-- Tests -->
  <ItemGroup>
    <PackageVersion Include="coverlet.collector" Version="6.0.2"/>
    <PackageVersion Include="Microsoft.NET.Test.Sdk" Version="17.11.1"/>
    <PackageVersion Include="xunit.v3" Version="0.3.0-pre.18"/>
    <PackageVersion Include="xunit.runner.console" Version="2.9.2"/>
    <PackageVersion Include="xunit.runner.visualstudio" Version="3.0.0-pre.35"/>
  </ItemGroup>

  <!-- Benchmarks -->
  <ItemGroup>
    <PackageVersion Include="BenchmarkDotNet" Version="0.14.0"/>
  </ItemGroup>

</Project>